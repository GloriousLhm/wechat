page {
  height: 100%;
}

.pressBtn {
  padding-top: 20px;
  background-color: pink;
  width: 740rpx;}

.header{
  height: 92rpx;
  background-color:#181818 
}
.city{
  width: 142rpx;
  text-align: center;
  line-height: 92rpx;
  color: #d3b9a0;
  font-size: 29rpx;
  float: left;
}
.city::after{
  content: "";
  display: inline-block;
  border-width: 10rpx 10rpx 0;
  border-style: solid;
  border-color: #d3b9a0 transparent transparent; 
  margin-left: 5rpx;
  vertical-align: middle;
  height: 0;
  position: relative;
  top: -2rpx;
}
.userinfo{
  float: right;
  width: 48rpx;
  height: 48rpx;
  margin: 28rpx 33rpx 0 0;
}
.userinfo image{
  width: 100%;
  height: 100%;

}
.nav-list{
  display: flex;
  height: 160rpx;
  background-color: #fff;
}
.nav-list navigator{
  margin: auto;
  flex: 1;
  text-align: center;

}
.nav-list image{
  width: 87.5rpx;
  height: 87.5rpx;
}
.nav-list text{
 display: block;
 line-height: 52rpx;
 font-size: 26rpx;
}
.list{
  margin-top: 20rpx;
  background-color: #fff;
}
.list .title{
  font:30rpx/80rpx "宋体"

}